<?xml version="1.0"?>
<robot name="frame_wall">
  <!-- Left Section -->
  <link name="left_section">
    <inertial>
      <origin xyz="0 -2 -4" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.083333" ixy="0" ixz="0" iyy="0.083333" iyz="0" izz="0.001667"/>
    </inertial>
    <visual>
      <origin xyz="0 -2 -4" rpy="0 0 0"/>
      <geometry>
        <box size="1.5 12 0.2"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 -2 -4" rpy="0 0 0"/>
      <geometry>
        <box size="1.5 12 0.2"/>
      </geometry>
    </collision>
  </link>

  <!-- Right Section -->
  <link name="right_section">
    <inertial>
      <origin xyz="0 -2 4" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.083333" ixy="0" ixz="0" iyy="0.083333" iyz="0" izz="0.001667"/>
    </inertial>
    <visual>
      <origin xyz="0 -2 4" rpy="0 0 0"/>
      <geometry>
        <box size="1.5 12 0.2"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 -2 4" rpy="0 0 0"/>
      <geometry>
        <box size="1.5 12 0.2"/>
      </geometry>
    </collision>
  </link>

  <!-- Top Section -->
  <link name="top_section">
    <inertial>
      <origin xyz="0 4 0" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.083333" ixy="0" ixz="0" iyy="0.083333" iyz="0" izz="0.001667"/>
    </inertial>
    <visual>
      <origin xyz="0 4 0" rpy="0 0 0"/>
      <geometry>
        <box size="1.5 0.2 8"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 4 0" rpy="0 0 0"/>
      <geometry>
        <box size="1.5 0.2 8"/>
      </geometry>
    </collision>
  </link>


  <!-- left to right -->
  <joint name="left_to_right" type="fixed">
    <parent link="left_section"/>
    <child link="right_section"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- top to Left -->
  <joint name="top_to_left" type="fixed">
    <parent link="top_section"/>
    <child link="left_section"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>



  
</robot>